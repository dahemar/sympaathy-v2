{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "framework": "vite",
  "headers": [
    {
      "source": "/posts_bootstrap.json",
      "headers": [
          { 
            "key": "Cache-Control",
            "value": "public, max-age=0, s-maxage=86400, stale-while-revalidate=86400"
          }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
